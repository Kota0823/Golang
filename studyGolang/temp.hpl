<html>
  <h1>RelayTable("html/template" package)</h1>
  <table border="1">
    RS address:
     <tr>
         <th>ID</th>
         <th>En1 address</th>
         <th>En2 address</th>
    </tr>
    {{/* ループ処理 */}}
    {{range .}}
     <tr>
         <th>{{.Index}}</th>
         <th>{{.En1}}</th>
         <th>{{.En2}}</th>
     </tr>
     {{end}}
     {{/* ループ処理終了 */}}
         
   </table>